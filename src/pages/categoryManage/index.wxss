.popup-tag-main {
  width: 100%;
  height: 100%;
}

.popup-tag-main .tag-top-bottom {
  width: 100%;
  position: relative;
  height: 80rpx;
  line-height: 80rpx;
  /* background-color: #fff */
}

.popup-tag-main .tag-top-bottom .popup-tag-close {
  padding-left: 30rpx;
  padding-right: 30rpx;
  position: absolute;
  display: inline-block;
  left: 0;
  color: #ee0a24;
  font-size: 14px;
}

.popup-tag-main .tag-top-bottom .popup-tag-ok {
  padding-left: 30rpx;
  padding-right: 30rpx;
  position: absolute;
  display: inline-block;
  right: 0;
  color: #ee0a24;
  font-size: 14px;
}

.popup-tag-main .popup-tag-list {
  margin-top: 20rpx;
  width: 100%;
}

.popup-tag-main .custom-button-tag {
  margin-left: 20rpx;
}

.popup-tag-main .custom-van-tag {
  margin-left: 20rpx;
  margin-top: 20rpx;
  font-size: 30rpx;
}

.van-tree-select {
  height: 100% !important;
}
