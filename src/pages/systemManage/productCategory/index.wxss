.sku-image-row .sku-upload-image {
  width: 130rpx;
  height: 130rpx;
  margin-top: 7rpx;
  float: left;
}

.sku-image-row .sku-upload-image .add-but:active {
  background-color: #f2f3f5;
}

.sku-image-row .sku-upload-image .add-but {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  /*Ã¦Â°Â´Ã¥Â¹Â³Ã¥Â±â€¦Ã¤Â¸Â­*/
  background-color: #fff;
}

.sku-image-row .sku-upload-image .v-block {
  width: 100%;
  height: 100%;
  background-color: #f2f3f5;
  position: relative;
}

.sku-image-row .sku-upload-image .v-block .v-block-image {
  width: 100%;
  height: 100%;
}

.sku-image-row .sku-upload-image .v-block .close {
  background-color: red;
  color: #fff;
  text-align: center;
  width: 25rpx;
  height: 28rpx;
  line-height: 25rpx;
  position: absolute;
  top: 0;
  right: 0;
}

.sku-image-row .hint-text {
  color: #969799;
  font-size: 14px;
  float: right;
  right: 0;
  height: 130rpx;
  line-height: 130rpx;
  display: inherit-block;
}