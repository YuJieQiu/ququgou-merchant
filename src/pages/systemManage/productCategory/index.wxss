Page {
  padding-top: 50rpx;
  padding-bottom: 1rpx;
}

.basic-info .basic-info-images {
  margin-bottom: 30rpx;
}

.basic-info {
  z-index: 50;
}

Page .ca-vi {
  margin-bottom: 20rpx;
}

Page .ca-ch-vi {
  padding-left: 50rpx;
  background: #f7f7f7;
}

.basic-info .basic-info-images .basic-info-images-guid .grid-item {
  margin: 0;
  padding: 0;
  border: none;
  height: 477rpx;
}

.basic-info .basic-info-images .basic-info-images-guid .grid-item .item-images {
  width: 95%;
  height: 302rpx;
}

.basic-info .basic-info-images .basic-info-images-guid .grid-item .close {
  right: 0px;
  bottom: 0px;
  width: 95%;
  height: 50rpx;
  margin-top: 5rpx;
}

.basic-info .basic-info-images .basic-info-images-guid .grid-item .grid-item-empty {
  width: 95%;
  height: 100%;
}

.basic-info .basic-info-images .basic-info-images-guid .grid-item .grid-item-add {
  background-color: #fff;
  width: 95%;
  height: 100%;
  position: relative;
}

.basic-info .basic-info-images .basic-info-images-guid .grid-item .grid-item-add .add {
  position: absolute;
  top: 0;
  bottom: 0;
  right: 0;
  left: 0;
  height: 50rpx;
  width: 50rpx;
  margin: auto;
}

.basic-info .basic-info-images .basic-info-images-guid .grid-item .grid-item-add:active {
  background-color: #f2f3f5;
}

Page .i-swipeout-demo-item {
  padding: 0 !important;
  margin: 0;
  border: 0;
}

Page .cus-collapse {
  padding: 0;
  margin: 0;
  border: 0;
}

Page .i-swipeout-item {
  padding: 0px !important;
}

Page .i-swipeout-wrap {
  padding: 0px !important;
}

